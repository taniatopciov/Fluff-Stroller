<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <Button
        android:id="@+id/ownerHomeButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="32dp"
        android:text="Owner Home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ownerHomeWaitingButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Owner Home Waiting"
        app:layout_constraintEnd_toEndOf="@+id/ownerHomeButton"
        app:layout_constraintStart_toStartOf="@+id/ownerHomeButton"
        app:layout_constraintTop_toBottomOf="@+id/ownerHomeButton" />

    <Button
        android:id="@+id/ownerHomeNoDogsButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:text="Owner Home No Dogs"
        app:layout_constraintEnd_toEndOf="@+id/ownerHomeButton"
        app:layout_constraintHorizontal_bias="0.514"
        app:layout_constraintStart_toStartOf="@+id/ownerHomeButton"
        app:layout_constraintTop_toBottomOf="@+id/ownerHomeWaitingButton" />
</androidx.constraintlayout.widget.ConstraintLayout>
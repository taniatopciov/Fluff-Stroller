<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout8"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".pages.profile.dogowner.ViewDogOwnerProfileFragment" >

    <TextView
        android:id="@+id/nameLabelViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="48dp"
        android:text="@string/name_label"
        android:textColor="@color/primary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/emailLabelViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:text="@string/email_label"
        android:textColor="@color/primary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nameLabelViewOwnerProfile" />

    <TextView
        android:id="@+id/phoneNumberLabelViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="24dp"
        android:text="@string/phone_number"
        android:textColor="@color/primary"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/emailLabelViewOwnerProfile" />

    <TextView
        android:id="@+id/nameTextViewViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/text"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/nameLabelViewOwnerProfile"
        app:layout_constraintStart_toEndOf="@+id/nameLabelViewOwnerProfile"
        app:layout_constraintTop_toTopOf="@+id/nameLabelViewOwnerProfile" />

    <TextView
        android:id="@+id/emailTextViewViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/text"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/emailLabelViewOwnerProfile"
        app:layout_constraintStart_toEndOf="@+id/emailLabelViewOwnerProfile"
        app:layout_constraintTop_toTopOf="@+id/emailLabelViewOwnerProfile" />

    <TextView
        android:id="@+id/phoneNumberTextViewViewOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="@string/text"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/phoneNumberLabelViewOwnerProfile"
        app:layout_constraintStart_toEndOf="@+id/phoneNumberLabelViewOwnerProfile"
        app:layout_constraintTop_toTopOf="@+id/phoneNumberLabelViewOwnerProfile" />

    <TextView
        android:id="@+id/myDogsLabelViewDogOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="48dp"
        android:layout_marginTop="48dp"
        android:text="@string/my_dogs"
        android:textColor="@color/primary"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phoneNumberLabelViewOwnerProfile" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerViewDogsViewDogOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/myDogsLabelViewDogOwnerProfile"
        app:layout_constraintVertical_bias="0.0" />

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/editProfileButtonViewDogOwnerProfile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="24dp"
        android:clickable="true"
        android:src="@drawable/ic_baseline_edit_24"
        app:fabSize="mini"
        app:layout_constraintBottom_toBottomOf="@+id/nameLabelViewOwnerProfile"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/nameLabelViewOwnerProfile"
        app:tint="@color/white" />
</androidx.constraintlayout.widget.ConstraintLayout>